<?xml version="1.0" encoding="utf-8"?>
<Assets>
    <Materials>
        <Material>
            <Name>ball01</Name>
            <Texture>
                <Albedo>ball01_albedo.png</Albedo>
            </Texture>
            <Uniform>
                <Metallic>0</Metallic>
                <Roughness>0.3</Roughness>
            </Uniform>
        </Material>

        <Material>
            <Name>beachSand</Name>
            <Texture>
                <Albedo>beachSand_albedo.png</Albedo>
                <AO>beachSand_ao.png</AO>
                <Depth>beachSand_depth.png</Depth>
                <Metallic>beachSand_metal.png</Metallic>
                <Normal>beachSand_norm.png</Normal>
                <Roughness>beachSand_rough.png</Roughness>
                <Parameters>
                    <MinFilter>nearest_mipmap_linear</MinFilter>
                    <MagFilter>linear</MagFilter>
                    <AddressU>wrap</AddressU>
                    <AddressV>wrap</AddressV>
                </Parameters>
            </Texture>
            <Uniform>
                <ParallaxHeightScale>0.10</ParallaxHeightScale>
            </Uniform>
            <Override>
                <NormalMap>true</NormalMap>
                <ParallaxMap>false</ParallaxMap>
            </Override>
        </Material>

        <Material>
            <Name>brickWall</Name>
            <Texture>
                <Albedo>brickWall_albedo.png</Albedo>
            </Texture>
        </Material>

        <Material>
            <Name>cube</Name>
            <Texture>
                <Albedo>cube_albedo.png</Albedo>
                <Metallic>cube_metal.png</Metallic>
                <Roughness>cube_rough.png</Roughness>
            </Texture>
        </Material>

        <Material>
            <Name>default</Name>
            <Uniform>
                <Albedo>(1,0,0)</Albedo>
                <Metallic>0</Metallic>
                <Roughness>1.0</Roughness>
            </Uniform>
        </Material>

        <Material>
            <Name>dome01</Name>
            <Texture>
                <Albedo>dome01_albedo.png</Albedo>
            </Texture>
            <Uniform>
                <Metallic>0</Metallic>
                <Roughness>0.1</Roughness>
            </Uniform>
        </Material>

        <Material>
            <Name>pavedFloor</Name>
            <Texture>
                <Albedo>pavedFloor_albedo.png</Albedo>
                <AO>pavedFloor_ao.png</AO>
                <Depth>pavedFloor_depth.png</Depth>
                <Metallic>pavedFloor_metal.png</Metallic>
                <Normal>pavedFloor_norm.png</Normal>
                <Roughness>pavedFloor_rough.png</Roughness>
            </Texture>
            <Uniform>
                <ParallaxHeightScale>0.15</ParallaxHeightScale>
            </Uniform>
            <Override>
                <NormalMap>true</NormalMap>
                <ParallaxMap>true</ParallaxMap>
            </Override>
        </Material>

        <Material>
            <Name>sandstone</Name>
            <Texture>
                <Albedo>sandstone_albedo.png</Albedo>
                <AO>sandstone_ao.png</AO>
                <Depth>sandstone_depth.png</Depth>
                <Metallic>sandstone_metal.png</Metallic>
                <Normal>sandstone_norm.png</Normal>
                <Roughness>sandstone_rough.png</Roughness>
            </Texture>
            <Uniform>
                <ParallaxHeightScale>0.15</ParallaxHeightScale>
            </Uniform>
            <Override>
                <NormalMap>true</NormalMap>
                <ParallaxMap>true</ParallaxMap>
            </Override>
        </Material>
    </Materials>

    <Meshes>
        <Mesh name=".tree01" type="tree">
            <Generator>
                <Seed>8</Seed>
                <Recursion>2</Recursion>
                <NodeProbability>0.7</NodeProbability>
                <BranchProbability>0.7</BranchProbability>
                <Twist>0.02</Twist>
                <MaxBranch>2</MaxBranch>
                <BranchAngle>0.3</BranchAngle>
                <BranchHindrance>0.1</BranchHindrance>
                <ScaleExponent>1.5</ScaleExponent>
                <RadiusExponent>0.8</RadiusExponent>
                <TrunkRadius>0.1</TrunkRadius>
                <MinSamples>4</MinSamples>
                <MaxSamples>10</MaxSamples>
                <MinSections>5</MinSections>
                <MaxSections>15</MaxSections>
                <MaxNodes>8</MaxNodes>
            </Generator>
        </Mesh>
        <Mesh name=".tree02" type="tree">
            <Generator>
                <Seed>10</Seed>
                <Recursion>2</Recursion>
                <NodeProbability>0.5</NodeProbability>
                <BranchProbability>0.5</BranchProbability>
                <Twist>0.1</Twist>
                <MaxBranch>3</MaxBranch>
                <BranchAngle>0.5</BranchAngle>
                <BranchHindrance>0.8</BranchHindrance>
                <ScaleExponent>1.8</ScaleExponent>
                <RadiusExponent>0.9</RadiusExponent>
                <TrunkRadius>0.24</TrunkRadius>
                <MinSamples>3</MinSamples>
                <MaxSamples>10</MaxSamples>
                <MinSections>3</MinSections>
                <MaxSections>15</MaxSections>
                <MaxNodes>8</MaxNodes>
            </Generator>
        </Mesh>
        <Mesh name=".tree03" type="tree">
            <Generator>
                <Seed>21</Seed>
                <Recursion>3</Recursion>
                <NodeProbability>0.4</NodeProbability>
                <BranchProbability>0.5</BranchProbability>
                <Twist>0.1</Twist>
                <MaxBranch>4</MaxBranch>
                <BranchAngle>0.6</BranchAngle>
                <BranchHindrance>0.8</BranchHindrance>
                <ScaleExponent>1.8</ScaleExponent>
                <RadiusExponent>0.5</RadiusExponent>
                <TrunkRadius>0.30</TrunkRadius>
                <MinSamples>3</MinSamples>
                <MaxSamples>15</MaxSamples>
                <MinSections>3</MinSections>
                <MaxSections>15</MaxSections>
                <MaxNodes>8</MaxNodes>
            </Generator>
        </Mesh>
        <Mesh name=".wall01" type="box">
            <Generator>
                <xmin>-0.125</xmin>
                <xmax>0.125</xmax>
                <ymin>0.0</ymin>
                <ymax>2.0</ymax>
                <zmin>-1.0</zmin>
                <zmax>1.0</zmax>
            </Generator>
        </Mesh>
    </Meshes>
</Assets>
